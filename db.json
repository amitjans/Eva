          {
            "key": 1598747582841,
            "name": "Hablar_4",
            "type": "speak",
            "text": "Me alegro mucho.",
            "color": "lightblue",
            "isGroup": false,
            "__gohashid": 2654
          },
          {
            "key": 1598747607791,
            "name": "Emoción_5",
            "type": "emotion",
